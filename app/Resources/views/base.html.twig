<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}Aaaa{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/vendor/font-awesome/css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/vendor/Ionicons/css/ionicons.min.css') }}">
        <link href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('assets/vendor/AdminLTE/dist/css/AdminLTE.min.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('assets/vendor/AdminLTE/dist/css/skins/_all-skins.min.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="{{ asset('assets/vendor/html5shiv/dist/html5shiv.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/respond/dest/respond.min.js') }}"></script>
    <![endif]-->

</head>

{% block body %}
    <body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        {% block header %}{% endblock %}

        {% block main_sidebar %}{% endblock %}

        {% block flash %}{% endblock %}

        {% block content_wrapper %}
            {% block breadcrumbs %}{% endblock %}
            {% block content %}{% endblock %}
        {% endblock %}

        {% block footer %}
            <footer class="main-footer">
                Copyright &copy; {{ "now"|date("Y") }} <strong>aaaa</strong>. All rights reserved.
            </footer>
        {% endblock %}
    </div>
    <!-- ./wrapper -->

    {% block javascripts %}
        <script src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
        <script src="{{ asset('assets/vendor/jquery-ui/jquery-ui.js') }}"></script>
        <script src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('assets/vendor/AdminLTE/dist/js/app.js') }}"></script>
    {% endblock %}

    </body>
{% endblock %}

</html>
